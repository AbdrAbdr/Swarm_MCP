{
  "name": "mcp-swarm",
  "version": "1.0.4",
  "private": false,
  "type": "module",
  "description": "Universal AI agent coordination platform with 26 consolidated smart tools, 21 AI models (Kimi K2.5, GPT-5.3, Opus 4.6), EWC++ self-learning, semantic caching, and Q-Learning routing",
  "keywords": [
    "mcp",
    "ai",
    "agent",
    "swarm",
    "coordination",
    "cloudflare"
  ],
  "author": "MCP Swarm Team",
  "license": "MIT",
  "homepage": "https://github.com/AbdrAbdr/MCP-Swarm#readme",
  "bugs": {
    "url": "https://github.com/AbdrAbdr/MCP-Swarm/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AbdrAbdr/MCP-Swarm.git"
  },
  "bin": {
    "mcp-swarm": "./dist/serverSmart.js",
    "mcp-swarm-remote": "./dist/remote/index.js",
    "mcp-swarm-companion": "./dist/companion.js",
    "mcp-swarm-install": "./dist/installer/index.js"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "node --enable-source-maps dist/serverSmart.js",
    "dev:legacy": "node --enable-source-maps dist/server.js",
    "remote": "node --enable-source-maps dist/remote/index.js",
    "companion": "node --enable-source-maps dist/companion.js",
    "bridge": "node --enable-source-maps dist/bridge.js",
    "install-mcp": "node --enable-source-maps dist/scripts/install.js",
    "dashboard:api": "node --enable-source-maps dist/dashboardApi.js",
    "dashboard:ui": "cd dashboard && npm run dev",
    "dashboard:install": "cd dashboard && npm install",
    "dashboard": "concurrently \"npm run dashboard:api\" \"npm run dashboard:ui\"",
    "dashboard:build": "cd dashboard && npm run build",
    "test": "node --enable-source-maps dist/tests/newModules.test.js"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "ws": "^8.18.0",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20260131.0",
    "@types/node": "^22.10.2",
    "@types/ws": "^8.18.0",
    "concurrently": "^8.2.2",
    "typescript": "^5.6.3"
  }
}